{
  "name": "data-service",
  "version": "0.1.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "mocha --timeout 10000",
    "lint": "eslint src"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "compression": "^1.5.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.7.1",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "mongodb": "^2.2.12",
    "morgan": "~1.7.0",
    "resource-router-middleware": "^0.5.1",
    "serve-favicon": "~2.3.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "frisby": "^0.8.5",
    "jasmine": "^2.5.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
